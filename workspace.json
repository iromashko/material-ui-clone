{
  "version": 2,
  "projects": {
    "material": {
      "projectType": "application",
      "root": "apps/material",
      "sourceRoot": "apps/material/src",
      "prefix": "mui",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/material",
            "index": "apps/material/src/index.html",
            "main": "apps/material/src/main.ts",
            "polyfills": "apps/material/src/polyfills.ts",
            "tsConfig": "apps/material/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/material/src/favicon.ico",
              "apps/material/src/assets"
            ],
            "styles": [
              "apps/material/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/material/src/environments/environment.ts",
                  "with": "apps/material/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "material:build:production"
            },
            "development": {
              "browserTarget": "material:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "material:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/material/src/**/*.ts",
              "apps/material/src/**/*.html"
            ]
          }
        }
      }
    },
    "material-e2e": {
      "root": "apps/material-e2e",
      "sourceRoot": "apps/material-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/material-e2e/cypress.json",
            "tsConfig": "apps/material-e2e/tsconfig.e2e.json",
            "devServerTarget": "material:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "material:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/material-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "shared-ui-accordion": {
      "projectType": "library",
      "root": "libs/shared/ui-accordion",
      "sourceRoot": "libs/shared/ui-accordion/src",
      "prefix": "accordion",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:ng-packagr-lite",
          "outputs": [
            "dist/libs/shared/ui-accordion"
          ],
          "options": {
            "project": "libs/shared/ui-accordion/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/shared/ui-accordion/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/shared/ui-accordion/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared/ui-accordion"
          ],
          "options": {
            "jestConfig": "libs/shared/ui-accordion/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/ui-accordion/src/**/*.ts",
              "libs/shared/ui-accordion/src/**/*.html"
            ]
          }
        }
      }
    },
    "shared-ui-card": {
      "projectType": "library",
      "root": "libs/shared/ui-card",
      "sourceRoot": "libs/shared/ui-card/src",
      "prefix": "card",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:ng-packagr-lite",
          "outputs": [
            "dist/libs/shared/ui-card"
          ],
          "options": {
            "project": "libs/shared/ui-card/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/shared/ui-card/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/shared/ui-card/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared/ui-card"
          ],
          "options": {
            "jestConfig": "libs/shared/ui-card/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/ui-card/src/**/*.ts",
              "libs/shared/ui-card/src/**/*.html"
            ]
          }
        }
      }
    },
    "shared-ui-progress-bar": {
      "projectType": "library",
      "root": "libs/shared/ui-progress-bar",
      "sourceRoot": "libs/shared/ui-progress-bar/src",
      "prefix": "progress-bar",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:ng-packagr-lite",
          "outputs": [
            "dist/libs/shared/ui-progress-bar"
          ],
          "options": {
            "project": "libs/shared/ui-progress-bar/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/shared/ui-progress-bar/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/shared/ui-progress-bar/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared/ui-progress-bar"
          ],
          "options": {
            "jestConfig": "libs/shared/ui-progress-bar/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/ui-progress-bar/src/**/*.ts",
              "libs/shared/ui-progress-bar/src/**/*.html"
            ]
          }
        }
      }
    },
    "shared-ui-toolbar": {
      "projectType": "library",
      "root": "libs/shared/ui-toolbar",
      "sourceRoot": "libs/shared/ui-toolbar/src",
      "prefix": "toolbar",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:ng-packagr-lite",
          "outputs": [
            "dist/libs/shared/ui-toolbar"
          ],
          "options": {
            "project": "libs/shared/ui-toolbar/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/shared/ui-toolbar/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/shared/ui-toolbar/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared/ui-toolbar"
          ],
          "options": {
            "jestConfig": "libs/shared/ui-toolbar/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/ui-toolbar/src/**/*.ts",
              "libs/shared/ui-toolbar/src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "none",
      "e2eTestRunner": "none"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "none"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  }
}